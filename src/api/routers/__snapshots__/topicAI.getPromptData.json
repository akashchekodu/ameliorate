{
  "schemas": {
    "createParts": {
      "type": "object",
      "properties": {
        "nodesToCreate": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "cause",
                  "problem",
                  "criterion",
                  "solutionComponent",
                  "benefit",
                  "effect",
                  "detriment",
                  "solution",
                  "obstacle",
                  "mitigationComponent",
                  "mitigation",
                  "question",
                  "answer",
                  "fact",
                  "source",
                  "rootClaim",
                  "support",
                  "critique",
                  "custom"
                ]
              },
              "text": {
                "type": "string",
                "maxLength": 200,
                "description": "Brief summary of the node's meaning. Should generally be concise and in clauses rather than complete sentences."
              },
              "notes": {
                "type": "string",
                "maxLength": 10000
              },
              "tempId": {
                "type": "number"
              }
            },
            "required": [
              "type",
              "text",
              "notes",
              "tempId"
            ],
            "additionalProperties": false
          }
        },
        "edgesToCreate": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "causes",
                  "subproblemOf",
                  "addresses",
                  "accomplishes",
                  "contingencyFor",
                  "createdBy",
                  "has",
                  "criterionFor",
                  "creates",
                  "fulfills",
                  "obstacleOf",
                  "mitigates",
                  "asksAbout",
                  "potentialAnswerTo",
                  "relevantFor",
                  "sourceOf",
                  "mentions",
                  "supports",
                  "critiques",
                  "relatesTo"
                ],
                "description": "Note: this unintuitively reads from the target node to the source node. E.g. target `Solution` node `addresses` source `Problem` node."
              },
              "notes": {
                "type": "string",
                "maxLength": 10000
              },
              "tempSourceId": {
                "type": "number"
              },
              "tempTargetId": {
                "type": "number"
              }
            },
            "required": [
              "type",
              "notes",
              "tempSourceId",
              "tempTargetId"
            ],
            "additionalProperties": false
          }
        }
      },
      "required": [
        "nodesToCreate",
        "edgesToCreate"
      ],
      "additionalProperties": false,
      "$schema": "http://json-schema.org/draft-07/schema#"
    }
  },
